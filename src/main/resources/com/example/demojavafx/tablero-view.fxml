<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demojavafx.TableroController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <TabPane tabClosingPolicy="UNAVAILABLE">
        <tabs>
            <Tab text="Tablero de juego">
                <Label text="Tablero del Juego" textAlignment="CENTER" textFill="RED">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <Label fx:id="welcomeText" />
                <GridPane fx:id="tableroDeJuego" />
            </Tab>
            <Tab text="Insertar individuo">
                <Label text="Tipo de individuo" />
                <ComboBox fx:id="individuosComboBox" prefWidth="200.0" />
                <Label text="Posicion en la que insertar"/>
            </Tab>
        </tabs>
    </TabPane>

</VBox>
